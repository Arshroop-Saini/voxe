{
  "name": "voxe-backend",
  "version": "1.0.0",
  "description": "Backend API server for Voxe - Voice-controlled productivity assistant",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec \"node --loader ts-node/esm ./src/index.ts\" --watch src --ext ts,js,json",
    "dev:ts": "ts-node-dev --respawn --transpile-only --esm src/index.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "voice",
    "ai",
    "productivity",
    "assistant"
  ],
  "author": "Voxe Team",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/elevenlabs": "^0.0.3",
    "@ai-sdk/openai": "^1.3.22",
    "@mem0/vercel-ai-provider": "^1.0.6",
    "@supabase/supabase-js": "^2.50.0",
    "@types/multer": "^1.4.13",
    "@types/uuid": "^10.0.0",
    "ai": "^4.3.16",
    "composio-core": "^0.5.39",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "nodemon": "^3.1.10",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
